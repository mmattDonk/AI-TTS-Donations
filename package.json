{
	"name": "solrock",
	"version": "1.0.0",
	"description": "https://solrock.mmattdonk.com/",
	"private": true,
	"packageManager": "pnpm@7.27.0",
	"scripts": {
		"backend": "pnpm --filter @solrock/backend run",
		"doc": "pnpm --filter @solrock/docs run",
